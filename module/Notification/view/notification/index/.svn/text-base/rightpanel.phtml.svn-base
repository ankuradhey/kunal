<script>
  function addToggleClass(type) {
        if (type == 'overdue') {
            $('#overdue').toggle('slow');
            $('#tschedule').hide();
        } else if (type == 'tschedule') {
            $('#overdue').hide();
            $('#tschedule').toggle('slow');
        }
    }
</script>
<script type="text/javascript">
    
    function sendFeedback(feedbackType){
        var hashStr = document.location.hash;
        var hashArr = hashStr.split('/');
        if('<?php echo $studentId;?>'==0) {
            var studentId = hashArr[2];
        } else {
            if('<?php echo $this->usertype;?>'=='1') {
            var studentId = '<?php echo $studentId;?>';
            } else {
                var studentId = '';
        }
        }
        
        if('<?php echo $classId;?>'==0) {
            var classId = hashArr[3];
        } else {
            if('<?php echo $this->usertype;?>'=='1') {
            var classId = '<?php echo $classId;?>';
            } else {
                classId ='';
        }
        }
        
        if(feedbackType=='performance') {
            if((studentId==0 || studentId=='' || typeof studentId == 'undefined') || (classId==0 || classId=='' || typeof classId == 'undefined')) {
                alert('Please Select Student From My Student');
            } else {
            var coverUrl = "<?php echo $this->basePath() ?>/report/json/get-coverage-report/"+studentId+"/"+classId;
            $.ajax({
                type: 'POST',
                dataType: 'json',
                url: coverUrl,
                success: function(data) {
                    var subjectId = data.coverage[0].id;
                    var performanceUrl = "<?php echo $this->basePath() ?>/report/index/test-history/"+studentId+"/"+subjectId;
                    location.href=performanceUrl;
                }
            });
            }
        } else if(feedbackType=='schedule') {
            if(hashArr[1]=='chapterDetails') {
                alert('Please Select Student From My Student');
            }if(studentId==0 || studentId=='' || typeof studentId == 'undefined') {
                alert('Please Select Student From My Student');
            } else {
            var urlPath = "<?php echo $this->basePath() ?>/scheduler-report/"+studentId;
            $.ajax({
                type: 'POST',
                dataType:'html',
                url: urlPath,
                success: function(data) {
                    $("#reminder_pop2").html(data);
                    $("#overlay").show();
                    $("#scheduler_reminder_modal").modal('show');
                }
            });
        }
        } else if(feedbackType=='coverage') {
            if((studentId==0 || studentId=='' || typeof studentId == 'undefined') || (classId==0 || classId=='' || typeof classId == 'undefined')) {
                alert('Please Select Student From My Student');
            } else {
            var coverUrl = "<?php echo $this->basePath() ?>/report/json/get-coverage-report/"+studentId+"/"+classId;
            $.ajax({
                type: 'POST',
                dataType: 'json',
                url: coverUrl,
                success: function(data) {
                    var subjectId = data.coverage[0].id;
                    var coverageUrl = "<?php echo $this->basePath() ?>/report/index/coverage-report#/subjectreport/"+studentId+"/"+classId+"/"+subjectId+"/";
                    location.href = coverageUrl;
                }
            });
        }
    }
    }
    $(function(){
        $('#close_schedule').click(function(){
            $("#overlay").hide();
            $("#scheduler_reminder_modal").modal('hide');
        });
    });
</script>
