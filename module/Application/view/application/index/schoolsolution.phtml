<script language='javascript' type='text/javascript'>
function refreshCaptcha(){
	$.ajax({
		type:'POST',
		url: BASE_URL+'/refresh-code',
		data:{refresh:'true'},
		success: function(data){ 
			if(data.output=='success'){
				$("#hidcheckvalue").val(data.securityCode);
				$("#secrity_refreshcode").val(data.securityCode);
			}
		}
	});
}
$(document).ready(function() {
	$('#feedbackfile').bind('change', function() {
		if(this.files[0].size > 2097152){
			$("#fileerror").val(1);
			alert("Image size should be below 2 mb");
			return false;
		}
		else
		{
			$("#fileerror").val(0);
		}
	});
	
});

</script>
