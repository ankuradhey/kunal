                        /*----Ashutosh----(after register redirection for custom pages)*/
                        $uri = $_SERVER['REQUEST_URI'];
                        //$controller = $this->getHelperPluginManager()->getServiceLocator()->get('application')->getMvcEvent()->getRouteMatch()->getParam('controller', 'index');
                        //$action = $this->getHelperPluginManager()->getServiceLocator()->get('application')->getMvcEvent()->getRouteMatch()->getParam('action', 'index'); 
                        if(strpos($uri,'career-assessment-program') != ''){
                            $loginRedirect = '/cap-package';
                        }else if(strpos($uri,'cap-package') != '' || strpos($uri,'career-counseling-test') != ''){
                            $loginRedirect = '/cap-package';
                        }else if(strpos($uri,'sat-exam-preparation') != ''){
                            $loginRedirect = '/diagnostic';
                        }else if(strpos($uri,'sat') != '' || strpos($uri,'vocabularybooster') != '' || strpos($uri,'fulllengthtest') != '' 
                                || strpos($uri,'performancetracker') != '' || strpos($uri,'practiceexercises') != ''){
                            $loginRedirect = '/diagnostic';
                        }else{
                            $loginRedirect = '';
                        }
