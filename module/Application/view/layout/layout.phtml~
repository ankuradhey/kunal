    <script>
        $(".navbar #cbse .popover").css("display", "block");
        $(".navbar .nav > li > a").hover(function() {
            var e = $(window).width();
            if (e <= 979) {
                $(".navbar .popover").css("left", "117px");
                $(".navbar .popover").css("top", "0px")
            } else {
                $(".navbar .popover").css("left", "137px");
                $(".navbar .popover").css("top", "0px")
            }
        });
        $(window).resize(function() {
            var e = $(window).width();
            if (e <= 979) {
                $(".navbar .popover").css("left", "117px");
                $(".navbar .popover").css("top", "0px")
            } else {
                $(".navbar .popover").css("left", "137px");
                $(".navbar .popover").css("top", "0px")
            }
        })
        $(".nav > li > a").css("outline", "none");
    </script>
