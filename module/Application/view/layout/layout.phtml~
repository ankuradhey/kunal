    <script>
$(document).ready(function() {
    $(".iconn").click(function() {
        $(".notifi_box_arrow").toggle();
        $(".notifi_box").toggle();
        $(".blank_div").toggleClass("blank_div_show");
        $("#notifiBox").toggleClass("highlight_zindex");
        $(".dropdown-menu").hide().toggleClass("highlight_zindex");
    });
    $(".blank_div").click(function() {
        $(".notifi_box_arrow").hide();
        $(".notifi_box").hide();
        $(".dropdown-menu").hide().removeClass("highlight_zindex");
        $(".blank_div").hide();
        $("#notifiBox").removeClass("highlight_zindex");
    });
});
    </script>
