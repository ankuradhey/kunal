    <script type="text/javascript">
        $(document).ready(function() {
            $("#country_name").change(function() {

                var country_code = $("#country_name").val();
                $('#imagechange').attr('src', '<?php echo $this->cdn() ?>/images/flags/' + country_code + '_32.png');
            });
            $(".study").click(function() {
                $("#boardclasscontainer").show();
            });
            $(".lavel2").click(function() {
                $('.popover-title').html($(this).html()); //For setting d Board Title
                $.ajax({type: 'POST',
                    url: BASE_URL + "/lms/index/containerlist/" + $(this).attr('id'),
                    // data: {rack_container_id: $(this).attr('id')},
                    dataType: 'json',
                    success: function(data) {
                        $('#classes').html('');
                        var i = 0;
                        var check2 = false;
                        var j = 0;
                        var classToUse = '';
                        var classEven = 'sub-hdlft1';
                        var classOdd = 'sub-hdlft2';
                        var flag = true;

                        $.each(data.options, function(key, obj)
                        {
                            if (check2)
                            {
                                if (j == 0 && j % 4 == 0)
                                {
                                    classEven = 'sub-hdlft2';
                                    classOdd = 'sub-hdlft1';
                                }
                                else if (j % 4 == 0)
                                {
                                    if (flag)
                                    {
                                        classEven = 'sub-hdlft1';
                                        classOdd = 'sub-hdlft2';
                                        flag = false;
                                    }
                                    else
                                    {
                                        classEven = 'sub-hdlft2';
                                        classOdd = 'sub-hdlft1';
                                        flag = true;
                                    }
                                }
                                if (j % 2 == 0)
                                    classToUse = classEven;
                                else
                                    classToUse = classOdd;
                                j++;
                            }

                            if (i == 0)
                                classToUse = 'sub-lfthd';
                            if (i == 1) {
                                classToUse = 'sub-rhthd';
                                check2 = true;
                            }
                            i++;
                            $(".board_listing ul li:odd").css("background-color", "#5484f7");
                            $('#classes').append('<div name="class_div" class="' + classToUse + '"><a href="' + BASE_URL + '/website/index/dashboard/' + obj.rack_id + '">' + obj.rack_name + '</a></div>');

                        }

                        )

                    }
                });


                $(".class_listing").show();
                $(".boardback").show(1000);
                $(".board_listing").animate({width: "0", }, 500);
                $(".class_listing").animate({marginLeft: "30px"}, 500);
            });

            $(".boardback").click(function() {
                $(".board_listing").animate({width: "200px", }, 500);
                //                    $('#classes').children().remove();
                $('[name=class_div]').remove();
                $(".class_listing").hide(500);
            });

            $("#mainSubjectBlockList").mCustomScrollbar({
                theme: "minimal"
            });

        });
        $(window).load(function()
        {
            //alert('mcustom');

        });
    </script>
