<script>
var disabledDays =<?php echo $this->totalDates;?>;
var date = new Date();
  $(function() {
	var page='<?php echo $this->page;?>';
	if(page=='myMentor'){
		$( "#txtDate").datepicker({ 
			showOn: "button",
			buttonImage: "<?php echo $this->cdn() ?>/images/website/calendar-icon1.png",
			buttonImageOnly: true,
			dateFormat: 'yy-mm-dd',
			numberOfMonths: 1,
			maxDate: 0,
			onSelect: function(selected,evnt) {
				$("#txtDate").datepicker("option","minDate", selected)
				updateAb(selected);
			},
			beforeShowDay: function(date) {
				var m = date.getMonth(), d = date.getDate(), y = date.getFullYear();
				for (i = 0; i < disabledDays.length; i++) {
					if($.inArray(y + '-' + (m+1) + '-' + d,disabledDays) != -1) {
						//return [false];
						return [true, 'Highlighted', ''];
					}
				}
				return [true];
			}
		});
	}else{
		$( "#txtDate1").datepicker({ 
			showOn: "button",
			buttonImage: "<?php echo $this->cdn() ?>/images/website/calendar-icon1.png",
			buttonImageOnly: true,
			dateFormat: 'yy-mm-dd',
			numberOfMonths: 1,
			maxDate: 0,
			onSelect: function(selected,evnt) {
				$("#txtDate1").datepicker("option","minDate", selected)
				updateAb(selected);
			},
			beforeShowDay: function(date) {
				var m = date.getMonth(), d = date.getDate(), y = date.getFullYear();
				for (i = 0; i < disabledDays.length; i++) {
					if($.inArray(y + '-' + (m+1) + '-' + d,disabledDays) != -1) {
						//return [false];
						return [true, 'Highlighted', ''];
					}
				}
				return [true];
			}
		});
	}
});

function updateAb(date){
	var page='<?php echo $this->page;?>';
	if(page=='myMentor'){
		$('#calenderLodings').show();
		var personType='student';
		var id=$('#feddMentorhid_id').val();
	}else{
		$('#calenderLoding').show();
		var personType='mentor';
		var id=$('#hid_student_id').val();
                var boardId=$('#hid_board_id').val();
		var studentName=$('#studentFeedbackName').val();
	}
	var subjectId='<?php echo $this->subject['subjectId']?>';
	var subjectName='<?php echo $this->subject['subjectName']?>';
	var class_id='<?php echo $this->subject['class_id']?>';
	$.ajax({
		type	:	'POST',
		url		: 	BASE_URL+'/account/ajax-feedback-student',
		data	:	{id:id,date:date,personType:personType,page:page,boardId:boardId,subjectId:subjectId,subjectName:subjectName,class_id:class_id},
		success	: 	function(data){
			$('.feedback1').html(data);
			$('#hidstudent_id').val(id);
			$('#studentFeedback').html(studentName);
			var splitDate=date.split('-');
			$('#selectedDate').html(splitDate[2]+'-'+splitDate[1]+'-'+splitDate[0]);
			if(page=='myStudent'){
				$('#calenderLoding').hide();
				$('#selectDate').val(date);
			}else{
				$('#calenderLodings').hide();
				var value=$('#checkStatus').val();
				if(value==0){
					var countN=$('#totalNotifications').html();
					var newN=parseInt(countN)-1;
					if(newN!=0){
						$('#totalNotifications').html(newN);
						$('#date'+date).hide();
					}else{
						$('#notifiBox').hide();
					}
				}
			}
			var feedId=$('#studentComment').val();
			if(feedId!=''){
				$.ajax({
					type	:	'POST',
					url		: 	BASE_URL+'/account/ajax_feedback_student_comments',
					data	:	{personType:personType,feedId:feedId},
					success	: 	function(data){
						var countN=$('#totalNotifications').html();
						var newN=parseInt(countN)-1;
						if(newN!=0){
							$('#totalNotifications').html(newN);
							$('#comment'+feedId).hide();
						}else{
							$('#notifiBox').hide();
						}
					}
				});
			}
		}
	});
}
