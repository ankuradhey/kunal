<script>
	function prevAndNextComments(type){
	$('#prevnextLoding').show();
	var value='';
	var nextValue=$('#hidNextPrevValue').val();
	if(type=='next'){
		value=parseInt(nextValue)+1;
	}else{
		value=parseInt(nextValue)-1;
	}
	var page='<?php echo $this->page;?>';
	if(page=='myMentor'){
		var id=$('#feddMentorhid_id').val();
		var personType='student';
	}else{
		var id=$('#hid_student_id').val();
                var boardId=$('#hid_board_id').val();
		var personType='mentor';
	}
	var feedback_id=$('#feedback_id').val();
	var subjectId='<?php echo $this->subject['subjectId']?>';
	var subjectName='<?php echo $this->subject['subjectName']?>';
	var class_id='<?php echo $this->subject['class_id']?>';
	$.ajax({
		type	:	'POST',
		url		: 	BASE_URL+'/account/ajax-feedback-student',
		data	:	{id:id,value:value,feedback_id:feedback_id,boardId: boardId,personType:personType,page:page,subjectId:subjectId,subjectName:subjectName,class_id:class_id},
		success	: 	function(data){
			$('.feedback1').html(data);
			$('#prevnextLoding').hide();
			if(page=='myStudent'){
				var studentName=$('#studentFeedbackName').val();
				$('#hidstudent_id').val(id);
				$('#studentFeedback').html(studentName);	
			}else{
				var date=$('#selectedDate').html();
				var splitDate=date.split('-');
				var presentDate=splitDate[2]+'-'+splitDate[1]+'-'+splitDate[0];
				var valueN=$('#checkStatus').val();
				if(valueN==0){
					var countN=$('#totalNotifications').html();
					var newN=parseInt(countN)-1;
					if(newN!=0){
						$('#totalNotifications').html(newN);
						$('#date'+presentDate).hide();
					}else{
						$('#notifiBox').hide();
					}
				}
			}
			$('#hidNextPrevValue').val(value);
			var totalComments=$('#totalComments').val();
			if(value==1){
				$('#nextFeed').show();
			}
			if(type=='next'){
				var hidnValue=value+1;
				if(hidnValue==totalComments){
					$('#prevFedd').hide();
					$('#nextFeed').show();
				}
			}
			if(type=='prev'){
				var hidpValue=value-2;
				if(hidpValue==totalComments){
					$('#prevFedd').show();
				}
				if(value==0){
					$('#nextFeed').hide();
				}
			}
			var feedId=$('#studentComment').val();
			if(feedId!=''){
				$.ajax({
					type	:	'POST',
					url		: 	BASE_URL+'/account/ajax_feedback_student_comments',
					data	:	{personType:personType,feedId:feedId},
					success	: 	function(data){
						var countN=$('#totalNotifications').html();
						var newN=parseInt(countN)-1;
						if(newN!=0){
							$('#totalNotifications').html(newN);
							$('#comment'+feedId).hide();
						}else{
							$('#notifiBox').hide();
						}
					}
				});
			}
		}
	});
}
function addMcqTests(value){
	var val=value+1;
	$('#tab'+value).hide();
	$('#MCQTests'+val).show();
	if((val+1)=='<?php echo $this->totalChapters?>'){
		$('#tab'+val).hide();
	}
	var incValue=value+1;
	$('#del'+incValue).show();
}
function delMcqTests(value){
	$('#feedbackTests'+value).val('');
	$('#MCQTests'+value).hide();
	var dirValue=value-1;
	$('#tab'+dirValue).show();
	$('#add'+dirValue).show();
	if(value==1){
		$('#del'+dirValue).hide();
	}else{
		$('#del'+dirValue).show();
	}
}
function addSubjectiveTest(value){
	var val=value+1;
	$('#tabs'+value).hide();
	$('#SubjectiveTest'+val).show();
	if((val+1)=='<?php echo $this->totalChapters?>'){
		$('#tabs'+val).hide();
	}
	var incValue=value+1;
	$('#delsub'+incValue).show();
}
function delubjectiveTest(value){
	$('#feedbackSubjective'+value).val('');
	$('#SubjectiveTest'+value).hide();
	var dirValue=value-1;
	$('#tabs'+dirValue).show();
	$('#addsub'+dirValue).show();
	if(value==1){
		$('#delsub'+dirValue).hide();
	}else{
		$('#delsub'+dirValue).show();
	}
}
