<script>
	function changeStatus(upAndDownId){
		$.ajax({
				type	:	'POST',
				url		: 	BASE_URL+'/account/change-status-downloads',
				data	:	{upAndDownId:upAndDownId},
				success	: 	function(data){
					var value=$('#checkStatus'+upAndDownId).val();
					if(value==0){
						var countN=$('#totalNotifications').html();
						var newN=parseInt(countN)-1;
						if(newN!=0){
							$('#totalNotifications').html(newN);
							$('#updown'+upAndDownId).hide();
						}else{
							$('#notifiBox').hide();
						}
					}
				}
			});
	}
	function hideAndshowUploads(type){
		if(type==1){
			$('#uploadsHideDiv').show();
			$('#uploadsHideshow').html('<a  onclick="hideAndshowUploads(0)" href="javascript:void(0)"><img src="<?php echo $this->cdn() ?>/images/website/minus.png"></a>New upload');
		}else{
			$('#uploadsHideDiv').hide();
			$('#uploadsHideshow').html('<a  onclick="hideAndshowUploads(1)" href="javascript:void(0)"><img src="<?php echo $this->cdn() ?>/images/website/plus.png"></a>New upload');
		}
	}
</script>
