    function validEmailData(host, type, userLogCount) {
    
    var flg = true;
    if ($("#email").val() == '') {
        $("#erroremail").css('display', 'block');
        $("#erroremail").html("Please enter email address");
        flg = false;
    }else if (checkEmail($("#email").val()) == false)
    {
        $("#erroremail").css('display', 'block');
        $('#erroremail').html('Please enter a valid email address');
        flg = false;
    } 
    else {
        $("#erroremail").css('display', 'none');
        $("#erroremail").html("");
    }

    if ($("#verifyEmail").val() == '') {
        $("#errorverifyEmail").css('display', 'block');
        $("#errorverifyEmail").html("Please verify email address");
        flg = false;
    }else if (checkEmail($("#verifyEmail").val()) == false)
    {
        $("#errorverifyEmail").css('display', 'block');
        $('#errorverifyEmail').html('Please enter a valid email address');
        flg = false;
    } 
    else if ($("#verifyEmail").val() != $("#email").val()) {
        $("#errorverifyEmail").css('display', 'block');
        $("#errorverifyEmail").html("Please verify email address");
        flg = false;
    } else {
        $("#errorverifyEmail").css('display', 'none');
        $("#errorverifyEmail").html("");
    }

    if (flg == false) {
        return false;
    } else {
        
        $.ajax({
            type: 'POST',
            dataType: 'json',
            url: BASE_URL + '/user/validemailsubmission',
            data: $("#addchild").serialize(),
            success: function(data) {
//                alert(data.output);
//                return false;
                if (data.output == 'emailExists') {
                    $("#errorverifyEmail").css('display', 'block');
                    $("#errorverifyEmail").html("Please change email Id");
                    return false;
                }
                else if (data.output == 'success') {
                    $(".overlay").css("display", "none");
                    $("#nursery-popup").css("display", "none");
                    window.location.href = BASE_URL + '/user/validemail';
                    return false;

                }else{
                    $("#errorverifyEmail").css('display', 'block');
                    $("#errorverifyEmail").html("Some error occurs");
                    return false;
                }

            }
        });
    }
}
