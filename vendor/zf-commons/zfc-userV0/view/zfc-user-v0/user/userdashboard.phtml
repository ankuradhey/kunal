            userrole:0
         }
         changeUserRole(data);

    } else {
        $(".dropdown-menu").show().addClass("highlight_zindex");
        $(".blank_div").show().removeClass("highlight_zindex");
        var data = {
            userrole:1
         }
          changeUserRole(data);
    }
}


function changeUserRole(data) {
        $.ajax({
            type: 'POST',
            url: '<?php echo $this->basePath(); ?>/user/changeuserrole',
            data: data,
            dataType: 'html',
            async: false,
            success: function(data) {//alert(data.key)
               return true;
            }
        });
    }

</script>


